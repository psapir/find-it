<!DOCTYPE HTML>
<html class="fuelux">
<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1.0">
<title>Find it!</title>
<link href="~/css/bootstrap.css" rel="stylesheet" type="text/css">
<link href="~/css/bootstrap-responsive.css" rel="stylesheet" type="text/css">

<link href="~/css/custom.css" rel="stylesheet" type="text/css">
<!--HTML5 shim for IE backwards compatibility -->
<!--[if lt IE 9]>
<script src="http://html5shim.googlecode.com/svn/trunk/html5.js">
</script>
<![endif]-->
</head>

<body>
<div class="container-fluid">

<div class="row-fluid">
  <div class="span12">
  
  
          <form>
          <fieldset>
           <div class="row-fluid">
           <div class="span12">
            <input type="text" placeholder="Type something…" class="searchy" id="search-input">
            <button type="submit" class="btn submitty" id="find-it">Submit</button>
            </div>
            </div>
            
            <div class="row-fluid">
            <div class="span12">
            
            <label class="checkbox inline">
              <input type="checkbox" id="checkbox-email"> email
            </label>
            
            
            <label class="checkbox inline">
              <input type="checkbox" id="checkbox-contentarea"> content area
            </label>
            
            <label class="checkbox inline" >
                  <input type="checkbox" id="checkbox-portfolio"> portfolio item
            </label>
          
            <label class="checkbox inline">
                  <input type="checkbox" id="checkbox-dataextension"> data extensions
            </label>
           
           
          </div>
         </div>
    
  </fieldset>
</form>
  <!--end container -->
  
  </div>
  </div>
  
  <div style="display:none;">
  
   <div class="row-fluid three-fourths">
    <div class="span12 three-fourths table-bordered padd20-bottom">
            
      
            <button type="text" class="button_edge_left" data-toggle="collapse" data-target="#demo" >
              Advance Search
            </button>
            <br clear="all">
            


            <div id="demo" class="collapse">
            
            <p class="text-info">Date Created:</p>
             From
             <input type="text" id="date-created-from">
             to
             <input type="text" id="date-created-to">
             <br>
            <p class="text-info"> Date Modified:</p>
             From
             <input type="text" id="date-modified-from">
             to
             <input type="text" id="date-modified-to">
            </div>
            
            
            
             <label class="checkbox inline">
                  <input type="checkbox" id="Customer-Key"> Look at Customer Key
            </label>
            <label class="checkbox inline">
                  <input type="checkbox" id="Case-Sensitive"> Case Sensitive
            </label>
            </div>
    </div>
    </div>


  <div class="row-fluid">
    <div class="span12">
    
    
    <!-- TABLE STARTS -->
    
     <!-- Tables
        ================================================== -->
        <section id="myresults">
        </section>
    <!--- TABLE ENDS --->
    
    </div>
   </div>

</div>
<script id="resultRow" type="text/x-handlebars-template">
          

          <h4>Result</h4>
          
          <div class="bs-docs-example">
            <table class="table table-bordered three-fourths altcolor">
              <thead>
                <tr>
                  <th>Filename</th>
                  <th>Customer Key</th>
                  <th>Date Created</th>
                  <th>Date Modified</th>
                  <th>Location</th>
                </tr>
              </thead>
              <tbody>
			  {{#each items}}
  <tr class="firstRow"><td>{{name}}</td><td>{{customerKey}}</td><td>{{createdDate}}</td><td>{{modifiedDate}}</td><td>{{Path}}</td></tr>
{{else}}
  <tr><td colspan="5">No results found</td></tr>
{{/each}}
              </tbody>
            </table>
          </div>
</script>
<script src="~/js/jquery.js">bootstrap.js</script>
<script src="~/js/bootstrap.min.js">bootstrap.js</script>
<script src="~/js/handlebars.js"></script>
<script src="~/js/findit.js"></script>
</body>
</html>
